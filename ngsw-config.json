{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/app/**/*.html",
          "/app/**/*.scss",
          "/app/**/*.ts",
          "/app/**/*.js",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    },
    {
      "name": "externals",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "urls": [
          "https://techcrunch.com/wp-content/uploads/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-sources",
      "urls": [
        "/v2/sources"
      ],
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "1d",
        "strategy": "performance"
      }
    },
    {
      "name": "api-top-articles",
      "urls": [
        "/v2/top-headlines"
      ],
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "1h",
        "timeout": "60s",
        "strategy": "freshness"
      }
    }
  ]
}
